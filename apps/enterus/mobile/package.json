{
  "name": "Enterus",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "codepush:and": "appcenter codepush release-react --entry-file ./index.tsx -a enterslash/enterus-1 -d Production",
    "codepush:ios": "appcenter codepush release-react --entry-file ./index.tsx -a enterslash/enterus -d Production",
    "build:and:apk": "node build-script.js --production && cd android && ./gradlew assembleRelease",
    "build:and:aab": "node build-script.js --production && cd android && ./gradlew bundleRelease",
    "build:script:prod": "node build-script.js --production",
    "start:and": "node build-script.js --android && react-native start",
    "start:ios": "node build-script.js --ios && react-native start"
  },
